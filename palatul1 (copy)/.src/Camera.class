Public Sub _new(unNr As Integer, oDenumire As String, unArtefact As String, usa0 As Integer, usa1 As Integer, usa2 As Integer, usa3 As Integer, nisteBani As Integer)
  With Me
    .Nr = unNr
    .Denumire = oDenumire
    .Artefact = unArtefact
    .Usa[0] = usa0
    .Usa[1] = usa1
    .Usa[2] = usa2
    .Usa[3] = usa3
    .Bani = nisteBani
  End With
End
